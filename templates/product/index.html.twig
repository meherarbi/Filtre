{% extends 'base.html.twig' %}

{% block title %}Mes Produits
{% endblock %}

{% block body %}
	<div class="container">
		<div class="row">
			<div class="col-md-3">
				{% include 'product/filter.html.twig' with {form:form} only %}
			</div>
			<div class="col-md-9">
				<div class="grid">
					{% for product in products %}
						{% include 'product/card.html.twig' with {product: product} only %}
					{% endfor %}
				</div>


			</div</div>

		{% endblock %}
